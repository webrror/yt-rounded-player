{
    "manifest_version": 3,
    "name": "YT-Rounded",
    "version": "0.1",
    "description": "An extension to add border radius to YT web player.",

    "content_scripts": [{
        "matches": [
            "*://youtube.com/*",
            "*://www.youtube.com/*",
            "*://m.youtube.com/*"
        ],
        "exclude_matches": ["*://*.music.youtube.com/*"],
        "css": ["my-style.css"],
        "run_at": "document_start"
    }]
}